    "build": "yarn pkg && yarn babel",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "joneldiablo",
  "license": "ISC",
  "dependencies": {
    "csvtojson": "^2.0.10",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "name": "pdform",
    "scripts": "index.js",
    "targets": [
